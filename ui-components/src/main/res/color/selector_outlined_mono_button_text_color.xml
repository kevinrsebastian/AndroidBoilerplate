<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled -->
    <item android:alpha="0.6" android:color="@color/outlined_mono_button_disabled" android:state_enabled="false" />
    <!-- Uncheckable -->
    <item android:color="@color/outlined_mono_button" android:state_checkable="false" />
    <!-- Checked Buttons. -->
    <item android:color="@color/outlined_mono_button" android:state_checked="true" />
    <!-- Not-checked Buttons. -->
    <item android:color="@color/outlined_mono_button" />
</selector>
